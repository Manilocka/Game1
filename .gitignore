# This .gitignore file is for Unity projects.

# Ignore common directories.
[Ll]ibrary/
[Tt]emp/
[Oo]bj/
[Bb]uild/
[Bb]uilds/

# Ignore generated files.
*.sln
*.csproj
*.unityproj
*.asmdef
*.apk
*.aab

# Ignore user-specific files.
*.pidb
*.suo
*.user
*.userprefs
*.prefs

# Ignore version control files.
.DS_Store

# Ignore log files.
*.log

# Ignore editor metafiles.
[Mm]etafiles/

# Ignore asset metafiles.
*.meta

# Ignore packages and project manifest.
[Pp]ackages/
[Mm]anifest.json

# Ignore lightmaps.
[Ll]ightmaps/

# Ignore light probes.
[Ll]ightProbes/

# Ignore the MonoDevelop directory.
[Mm]onoDevelop/

# Ignore the Test directory.
[Tt]est[Rr]esults/

# Ignore Xcode workspace files.
*.xcworkspace

# Ignore Visual Studio Code workspace files.
*.code-workspace

# Ignore JetBrains Rider workspace files.
.idea/

# Ignore Android Studio project files.
.gradle/